{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@components/*": "./components/*",
          "@lib/*": "./lib/*",
          "@styles/*": "./styles/*",
          "@pages/*": "./pages/*"
        }
      }
    ],
    ["styled-components", { "ssr": true }]
  ]
}
